{
  "name": "draw-tech-test",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "check-prop-types": "^1.1.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "expect": "^23.6.0",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-enzyme": "^6.1.2",
    "lint-staged": "^7.0.0",
    "moxios": "^0.4.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.11.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.5.2",
    "redux-mock-store": "^1.5.3",
    "sass-lint": "^1.12.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^4.3.0",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.10.0",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^1.0.17",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "request": "^2.88.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/styles/sass/ -o src/styles/css/",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/sass/ -o src/styles/css/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "eject": "react-scripts eject",
    "lint": "npm run eslint && npm run sasslint",
    "preeslint": "echo 'Running eslint'",
    "eslint": "eslint src",
    "presasslint": "echo 'Running sasslint'",
    "sasslint": "sass-lint -c .sass-lint.yml 'src/styles/sass/*.scss' -v --no-exit",
    "test": "jest --watch --verbose --coverage"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
